<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="550.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SearchController">
   <children>
      <TableView fx:id="tab" editable="true" layoutX="13.0" layoutY="47.0" prefHeight="138.0" prefWidth="272.0">
        <columns>
          <TableColumn id="name" fx:id="name" prefWidth="75.0" text="Name" />
          <TableColumn id="race" fx:id="race" prefWidth="75.0" text="Race" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="details" layoutX="14.0" layoutY="185.0" mnemonicParsing="false" onAction="#handleDetailsAction" text="Details" />
      <AnchorPane fx:id="rootPane" layoutX="-1.0" layoutY="220.0" prefHeight="330.0" prefWidth="300.0" />
   </children>
</AnchorPane>
