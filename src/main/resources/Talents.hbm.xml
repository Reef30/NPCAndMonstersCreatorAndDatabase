<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.mappings.Talents" table="talents" schema="wfrp4">
        <id name="talentId" column="talent_id"/>
        <property name="talentName" column="talent_name"/>
        <property name="description" column="description"/>
        <property name="maxTaken" column="max_taken"/>
    </class>
</hibernate-mapping>